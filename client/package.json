{
	"name": "php-ci",
  "displayName": "PHP intellisense for codeigniter",
	"description": "Code completion for PHP codeigniter framework",
	"author": "Small",
	"license": "MIT",
	"version": "0.0.9",
	"publisher":"small",
  "engines": {
    "vscode": "^1.7.0"
  },
  "categories": [
    "Other"
  ],
	"repository": {
		"type": "git",
		"url": "https://github.com/smallp/vscode-ci.git"
	},
	"bugs": {
		"url": "https://github.com/smallp/vscode-ci/issues"
	},
  "activationEvents": [
		"workspaceContains:/system/"
  ],
  "main": "./out/src/extension",
  "contributes": {
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "update-vscode": "node ./node_modules/vscode/bin/install",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.33",
    "@types/node": "^6.0.52",
    "typescript": "^2.1.4",
    "vscode": "^1.0.3"
  },
  "dependencies": {
    "vscode-languageclient": "^2.6.3"
  }
}
